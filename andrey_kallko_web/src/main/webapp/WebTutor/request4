request = '';

        request = XQuery('for $index in active_learnings return $index ');

           for(elem in request){

                open = OpenDoc(UrlFromDocID(elem.id));
                open.TopElem.course_name.HasValue?open.TopElem.course_name:'';
                //open.Save();

                }





        request = XQuery('for $index in learnings return $index ');

        for(elem in request) {

            open = OpenDoc(UrlFromDocId(elem.id));
            open.TopElem.course_name.HasValue?open.TopElem.course_name:'';
            open.TopElem.person_fullname.HasValue?open.TopElem.person_fullname:'';


        }



        request = XQuery('for $index in active_test_learnings return $index ');

        for(elem in request) {

               open = OpenDoc(UrlFromDocId(elem.id));
               open.TopElem.assessment_name.HasValue?open.TopElem.assessment_name:'';
               open.TopElem.person_fullname.HasValue?open.TopElem.person_fullname:'';
               open.TopElem.person_position_name.HasValue?open.TopElem.person_position_name:'';


        }



        request = XQuery('for $index in test_learnings return $index ');

        for(elem in request) {

                 open = OpenDoc(UrlFromDocId(elem.id));
                 open.TopElem.assessment_name.HasValue?open.TopElem.assessment_name:'';
                 open.TopElem.assessment_code.HasValue?open.TopElem.assessment_code:0;
                 open.TopElem.person_fullname.HasValue?open.TopElem.person_fullname:'';
                 open.TopElem.person_position_name.HasValue?open.TopElem.person_position_name:'';
                 open.TopElem.person_org_name.HasValue?open.TopElem.person_org_name:'';
                 open.TopElem.person_subdivision_name.HasValue?open.TopElem.person_subdivision_name:'';


        }