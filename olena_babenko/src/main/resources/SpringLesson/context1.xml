<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="company1" class="SpringLesson.Company">
        <constructor-arg value="companyName1"/>
        <constructor-arg ref="director1"/>
        <constructor-arg ref="car1"/>
    </bean>
    <bean id="director1" class="SpringLesson.Director">
        <property name="directorName" value="director1"/>
        <constructor-arg ref="car1"/>
    </bean>
    <bean id="car1" class="SpringLesson.Car">
        <property name="carName" value="car1"/>
    </bean>
</beans>